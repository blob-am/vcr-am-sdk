{
  "name": "@blob-solutions/vcr-am-sdk",
  "version": "0.7.0",
  "description": "Virtual Cash Register SDK for Armenia",
  "keywords": [
    "virtual cash register",
    "cash register",
    "armenia"
  ],
  "bugs": {
    "url": "https://github.com/blob-am/vcr-am-sdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blob-am/vcr-am-sdk.git"
  },
  "license": "ISC",
  "author": "Alex Kraiz",
  "type": "module",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "require": "./lib/index.cjs",
      "import": "./lib/index.js"
    },
    "./browser": {
      "browser": "./lib/index.global.js"
    }
  },
  "main": "./lib/index.cjs",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsup src/index.ts",
    "postinstall": "patch-package",
    "watch-ts": "tsc -w --skipLibCheck"
  },
  "dependencies": {
    "simple-typed-fetch": "^0.2.14",
    "typescript": "^5.6.3",
    "unfetch": "^5.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.4",
    "@tsconfig/strictest": "^2.0.5",
    "@types/node": "^20.14.2",
    "tsup": "^8.3.0",
    "type-fest": "^4.26.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "overrides": {
    "tsconfig-paths": "^4.0.0"
  }
}
